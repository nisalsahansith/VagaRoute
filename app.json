{
  "expo": {
    "name": "VagaRoute",
    "slug": "VagaRoute",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/vagaRoute_logo.png",
    "scheme": "vagaroute",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/vagaRoute_logo.png",
        "backgroundImage": "./assets/images/vagaRoute_logo.png",
        "monochromeImage": "./assets/images/vagaRoute_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "predictiveBackGestureEnabled": false,
      "package": "com.nisalsahan.VagaRoute",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBVH-_aQYd15yrDcoyUXYhiRBaAJH0XiTM"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/vagaRoute_logo.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f0017fd2-9cf7-4d66-af1a-fdc0d04ae30c"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f0017fd2-9cf7-4d66-af1a-fdc0d04ae30c"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
